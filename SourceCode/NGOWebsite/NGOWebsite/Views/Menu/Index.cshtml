                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@Url.Action("Index", "User")">
                    <img src="~/Content/View_components/images/logo_s_w.png" alt="logo"></a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="navfix navbar-nav navbar-right">
                    @if (Model != null)
                    {
                        foreach (var item in Model)
                        {
                            string str = item.Subject;
                            str = str.Split(new char[] { ' ' })[0];
                        <li><a href="@Url.Action(str, "User")">@item.Subject</a></li>
                        }
                    }
                   

