@model IEnumerable<Models.Informations>
@{
    ViewBag.Title = "WhatWeDo";
}
<style type="text/css">
    p {
        color: green;
    }

</style>

<div class="container">
    <div class="row">
        <div class="col-md-12 heading dark text-center animate_top">What  <span>we do</span></div>
        @foreach (var item in Model)
        {                    
            <div class="col-md-1"></div>
            <div class="col-md-10 text-center animate_bottom">              
                <p>@Html.Raw(item.Contents)</p>
            </div>
            <div class="col-md-1"></div>
        }
    </div>
</div>
